doctype html
html(lang="en")
    head
        meta(charset="UTF-8")
        meta(http-equiv="X-UA-Compatible", content="IE=edge")
        meta(name="viewport", content="width=device-width, initial-scale=1.0")
        title This is home page
    body 
        h1 Home page
        
        form(method='POST' id='urlForm')
            label(for='urlLabel') Enter URL
            br
            input#url(type='text' name='url')
            br
            button(class="btn")
                | Submit
        #container.col
            h2 #{finalURL}